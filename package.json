    "@ngrx/store": "^10.0.0",
    "@ngrx/store-devtools": "^10.0.0",
    "@ngx-translate/core": "^13.0.0",
    "@sentry/angular": "^5.24.2",
    "@sentry/tracing": "^5.24.2",
    "cordova-android": "^9.0.0",
    "cordova-custom-config": "^5.1.0",
    "cordova-ios": "^6.1.1",
    "cordova-plugin-device": "^2.0.3",
