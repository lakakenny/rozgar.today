    "@biesbjerg/ngx-translate-extract": "^7.0.2",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@compodoc/compodoc": "^1.1.11",
    "@ngx-rocket/scripts": "^4.0.0",
    "@types/jasmine": "^3.5.14",
    "@types/jasminewd2": "^2.0.8",
    "@types/jest": "^26.0.14",
    "@types/node": "^12.11.1",
